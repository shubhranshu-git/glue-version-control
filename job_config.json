{
  "name": "git_version_control_job",
  "description": "Sample job using Git version control",
  "role": "AWSGlueServiceRole",
  "command": {
    "name": "glueetl",
    "scriptLocation": "s3://aws-glue-assets-139097848366-us-east-1/scripts/",
    "pythonVersion": "3"
  },
  "sourceControl": {
    "provider": "GITHUB",
    "repository": "glue-version-control",
    "branch": "main",
    "folder": "src"
